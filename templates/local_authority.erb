<%
  not @section_name.empty? and @l_section_name = @section_name or @l_section_name = @name

  output = []
  output << '[' + @l_section_name + ']'
  output << 'Identity=' + Array(@identity).join(';')
  output << 'Action=' + @action
  @result_active and output << 'ResultActive=' + @result_active
  @result_inactive and output << 'ResultInactive=' + @result_inactive
  @result_any and output << 'ResultAny=' + @result_any
  @return_value and output << 'ReturnValue=' + @return_value
-%>
<%= output.join("\n") %>
